{
	"name": "SimpleBatchUpload",
	"version": "1.8.2",
	"author": [
		"[https://www.mediawiki.org/wiki/User:F.trott Stephan Gambke]",
		"[https://professional.wiki/ Professional.Wiki]",
		"..."
	],
	"url": "https://www.mediawiki.org/wiki/Extension:SimpleBatchUpload",
	"descriptionmsg": "simplebatchupload-desc",
	"namemsg": "simplebatchupload-name",
	"license-name": "GPL-2.0-or-later",
	"type": "specialpage",
	"requires": {
		"MediaWiki": ">=1.38.0"
	},
	"config": {
		"SimpleBatchUploadMaxFilesPerBatch": {
			"value":{
				"*": 1000
			}
		}
	},
	"MessagesDirs": {
		"SimpleBatchUpload": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"SimpleBatchUploadAlias" : "SimpleBatchUpload.alias.php",
		"SimpleBatchUploadMagic" : "SimpleBatchUpload.magic.php"
	},
	"AutoloadNamespaces": {
		"SimpleBatchUpload\\": "src/"
	},
	"SpecialPages": {
		"BatchUpload": "SimpleBatchUpload\\SpecialBatchUpload"
	},
	"Hooks": {
		"MakeGlobalVariablesScript": "main",
		"ParserFirstCallInit": "main"
	},
	"HookHandlers": {
		"main": {
			"class": "SimpleBatchUpload\\SimpleBatchUpload"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "SimpleBatchUpload"
	},
	"ResourceModules": {
		"ext.SimpleBatchUpload.jquery-file-upload": {
			"dependencies": [
				"jquery.ui"
			],
			"messages": [
				"simplebatchupload-comment",
				"simplebatchupload-max-files-alert"
			],
			"styles": [
				"res/jquery.fileupload.css"
			],
			"scripts": [
				"res/jquery.fileupload.js"
			]
		},
		"ext.SimpleBatchUpload": {
			"dependencies": [
				"ext.SimpleBatchUpload.jquery-file-upload",
				"mediawiki.Title",
				"mediawiki.api",
				"mediawiki.jqueryMsg"
			],
			"messages": [
				"simplebatchupload-comment",
				"simplebatchupload-max-files-alert"
			],
			"styles": [
				"res/ext.SimpleBatchUpload.css"
			],
			"scripts": [
				"res/ext.SimpleBatchUpload.js"
			]
		}
	},
	"manifest_version": 2
}
